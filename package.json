{
	"name": "statusbar-quotes",
	"displayName": "StatusBar Quotes",
	"description": "blah",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "StatusBar Quotes",
			"properties": {
				"statusbar-quotes.category": {
					"type": "string",
					"default": "Wise Saying",
					"enum": ["Wise Saying", "Humor"],
					"enumDescriptions": [
						"TODO",
						"TODO"
					],
					"description": "TODO"
				},
				"statusbar-quotes.language": {
					"type": "string",
					"default": "English",
					"enum": ["English", "Korean"],
					"enumDescriptions": [
						"Hello?",
						"World!"
					],
					"description": "TODO"
				}
			}
		},
		"commands": [
			{
				"command": "statusbar-quotes.select-language",
				"title": "Select Language",
				"category": "StatusBar Quote"
			},
			{
				"command": "statusbar-quotes.select-category",
				"title": "Select Category",
				"category": "StatusBar Quote"
			}
		]
	},
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
	"keywords": [
		"statusbar quote",
		"statusbar",
		"quote",
		"quotes",
		"good quote",
		"good quotes",
		"wise saying"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
